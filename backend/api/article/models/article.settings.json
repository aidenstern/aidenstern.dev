{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Date": {
      "type": "datetime"
    },
    "Body": {
      "type": "richtext"
    },
    "Description": {
      "type": "text"
    },
    "Title": {
      "type": "string"
    }
  }
}
